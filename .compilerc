{
  "env":{
     "development":{
        "application/javascript":{
           "presets":[
              [
                 "env",
                 {
                    "targets":{
                       "electron":"1.6.0"
                    },
                    "test":{
                       "plugins":[
                          "@babel/plugin-transform-runtime"
                       ]
                    }
                 }
              ],
              "react",
              "flow"
           ],
           "plugins":[
              "transform-async-to-generator",
              "transform-es2015-classes",
              "transform-object-rest-spread"
           ],
           "sourceMaps":"inline"
        }
     },
     "production":{
        "application/javascript":{
           "presets":[
              [
                 "env",
                 {
                    "targets":{
                       "electron":"1.6.0"
                    }
                 }
              ],
              "react",
              "flow"
           ],
           "plugins":[
              "transform-async-to-generator",
              "transform-es2015-classes",
              "transform-object-rest-spread"
           ],
           "sourceMaps":"none"
        }
     }
  }
}